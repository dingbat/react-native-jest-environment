{
  "name": "react-native-jest-environment",
  "version": "0.0.2",
  "description": "Lightweight Jest environment for use without a CLI",
  "main": "index.js",
  "scripts": {
    "unit": "node_modules/.bin/mocha --require babel-core/register",
    "cucumber": "node_modules/.bin/cucumber-js --compiler js:babel-register",
    "test": "npm run unit && npm run cucumber"
  },
  "dependencies": {
    "jest": "^17.0.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cucumber": "^2.0.0-rc.1",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
    "react-native": "^0.39.0",
    "react-test-renderer": "^15.4.1"
  },
  "author": "Dan Hassin",
  "license": "MIT",
  "url": "https://github.com/dingbat/react-native-jest-environment/issues",
  "repository": "dingbat/react-native-jest-environment",
  "keywords": [
    "react-native",
    "jest",
    "cucumber"
  ]
}
